[core]
	editor = vim
	excludesfile = /Users/alexanderfrankel/.gitignore
[alias]
  diffy = difftool --no-symlinks --dir-diff
[url "ssh://git@heroku.com/"]
	insteadOf = https://git.heroku.com/
[diff]
      tool = kitty
          guitool = kitty.gui
[difftool]
      prompt = false
          trustExitCode = true
[difftool "kitty"]
      cmd = kitty +kitten diff $LOCAL $REMOTE
[difftool "kitty.gui"]
      cmd = kitty kitty +kitten diff $LOCAL $REMOTE
